<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.alw.emarketshops">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.ActivityProducts"></activity>
        <activity android:name=".Activity.ActivitySubCategory" />
        <activity android:name=".Activity.ActivityCategory" />
        <activity android:name=".Activity.ActivityChat" />
        <activity android:name=".Activity.ActivityMyQuotation" />
        <activity android:name=".Activity.ActivityReQuotation" />
        <activity android:name=".Activity.ActivityQuotation" />
        <activity android:name=".Activity.ActivityQrthai" />
        <activity android:name=".Activity.ActivitySelectPayment" />
        <activity
            android:name=".Activity.ItemDetailActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.LoginActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.RegistrtionActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Activity.SplashActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.MainActivity"
            android:screenOrientation="portrait" />
    </application>

</manifest>